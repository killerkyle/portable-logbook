  <div class="card z-depth-5 card-main">
    <h3 class="card-header text-center font-weight-bold text-uppercase py-4 morpheus-den-gradient">Logbook</h3>
    <div class="card-body">
      <div id="table" class="table-editable">
        <span class="table-add float-right mb-3 mr-2"><a href="#!" class="text-success"><i
              class="fas fa-plus fa-2x" aria-hidden="true"></i></a></span>
        <table id="dataTable-1" class="table table-bordered table-responsive-md table-striped text-center">
          <thead>
            <tr>
              <th class="text-center">Contact Time (UTC)</th>
              <th class="text-center">Callsign</th>
              <th class="text-center">Signal Recieved</th>
              <th class="text-center">Signal Sent</th>
              <th class="text-center">Frequency</th>
              <th class="text-center">Mode</th>
              <th class="text-center">Contact Name</th>
              <th class="text-center">Contact Location</th>
              <th class="text-center">Notes</th>
            </tr>
          </thead>
          <tbody>
            {{#each logbook}}
            <tr>
              <td class="pt-3-half" contenteditable="true">{{contactTime}}</td>
              <td class="pt-3-half" contenteditable="true">{{callsign}}</td>
              <td class="pt-3-half" contenteditable="true">{{signalIn}}</td>
              <td class="pt-3-half" contenteditable="true">{{signalOut}}</td>
              <td class="pt-3-half" contenteditable="true">{{frequency}}</td>
              <td class="pt-3-half" contenteditable="true">{{mode}}</td>
              <td class="pt-3-half" contenteditable="true">{{contactName}}</td>
              <td class="pt-3-half" contenteditable="true">{{contactLocation}}</td>
              <td class="pt-3-half" contenteditable="true">{{contactNotes}}</td>
                <!-- <span class="table-up"><a href="#!" class="indigo-text"><i class="fas fa-long-arrow-alt-up"
                      aria-hidden="true"></i></a></span>
                <span class="table-down"><a href="#!" class="indigo-text"><i class="fas fa-long-arrow-alt-down"
                      aria-hidden="true"></i></a></span> -->
              <!-- <td>
                <span class="table-remove"><button type="button"
                    class="btn btn-danger btn-rounded btn-sm my-0">Remove</button></span>
              </td> -->
            </tr>
            {{/each}}
            <!-- This is our clonable table line -->
            <tr>
              <td class="pt-3-half" contenteditable="true"></td>
              <td class="pt-3-half" contenteditable="true"></td>
              <td class="pt-3-half" contenteditable="true"></td>
              <td class="pt-3-half" contenteditable="true"></td>
              <td class="pt-3-half" contenteditable="true"></td>
